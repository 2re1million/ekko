    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.business",
      "target": "dmg"
    }
  },
  "type": "commonjs",
  "dependencies": {
